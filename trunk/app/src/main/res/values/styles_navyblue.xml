    <style name="Theme.Navyblue.Widget" parent="@android:style/Theme.Holo">
        <item name="android:popupMenuStyle">@style/PopupMenu.Navyblue</item>
        <item name="android:dropDownListViewStyle">@style/DropDownListView.Navyblue</item>
    </style>
    -->

</resources>